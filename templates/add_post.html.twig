                <select class="form-control st_louis_categories_and_sub_category" name="catagory_id" id="category" onchange="subCategoryListOne(this);"> 
                    <option value="0" selected="selected">Select a category</option>
                    {% for category in category_result %}
                    <option value="{{ category.mnst_category.id }}">{{ category.mnst_category.category_name }}</option>
                                        {% endfor %}
                </select>
            </div>
        </div>
