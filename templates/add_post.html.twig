                                            <input id="wednesdaytohour" class="manast_open_hours_input_time stlouis-time-one" type="time" name="wednesday_usr_time_to">                                     
                                        </div>
                                    </div>    
                                </li>
                                <li class="col-md-4">
                                    <div class="manast_open_hours_box">
                                        <div class="manast_open_hours_left">
                                            <label for="thursday" class="manast_open_hours_label">THUR</label>
                                        </div>
                                        <div class="manast_open_hours_right">
                                            <input type="time" id="thursday" class="manast_open_hours_input_time stlouis-time-one" name="thursday_usr_time_from"> 
                                            <label for="thursdaytohour" class="manast_open_hours_label_to" >-</label>
                                            <input id="thursdaytohour" class="manast_open_hours_input_time stlouis-time-one" type="time" name="thursday_usr_time_to">
                                        </div>
                                    </div>
                                    <div class="manast_open_hours_box">
                                        <div class="manast_open_hours_left">
                                            <label for="friday" class="manast_open_hours_label">FRI</label>
                                        </div>
                                        <div class="manast_open_hours_right">
                                            <input type="time" id="friday" class="manast_open_hours_input_time stlouis-time-one" name="friday_usr_time_from"> 
                                            <label for="fridaytohour" class="manast_open_hours_label_to" >-</label>
                                            <input id="fridaytohour" class="manast_open_hours_input_time stlouis-time-one" type="time" name="friday_usr_time_to">
                                        </div>
                                    </div>
                                    <div class="manast_open_hours_box">
                                        <div class="manast_open_hours_left">    
                                            <label for="saturday" class="manast_open_hours_label">SAT</label>
                                        </div>
                                        <div class="manast_open_hours_right">    
                                            <input type="time" id="saturday" class="manast_open_hours_input_time stlouis-time-one" name="saturday_usr_time_from"> 
                                            <label for="saturdaytohour" class="manast_open_hours_label_to" >-</label>
                                            <input id="saturdaytohour" class="manast_open_hours_input_time stlouis-time-one" type="time" name="saturday_usr_time_to">        
                                        </div>
                                    </div>

                                </li>
                                <li class="col-md-4">
                                    <div class="manast_open_hours_box">
                                        <div class="manast_open_hours_left">
                                            <label for="sunday" class="manast_open_hours_label">SUN</label>
