<script>
    function postNameExist(obj) {
         var post_name = obj.value;
         var sub_catagory_id = $('#subcategories').val();

         if(post_name =='' && sub_catagory_id ==''){
            return false;
         }

         $.ajax({
            url: "{{ GLOBAL_DIR_PATH }}add-post.php/post_exist",
            type: "post",
            data: {"post_name":post_name,"sub_catagory_id":sub_catagory_id},
            success: function(response){
                var response = JSON.parse(response);
                if(response.existPost == 1){
                    $(".exist-post-error").html('sorry, this post already exits. please change post name');
                    
                }else{
                    $(".exist-post-error").html('');

                }
            },
            error: function(responce){
                alert('error');
            }
         });

     } 
</script>
